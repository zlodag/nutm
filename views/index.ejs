<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <script src="/bower/angular/angular.js"></script>
    <script src="/bower/angular-resource/angular-resource.js"></script>
    <script src="/bower/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="/angular/nutmApp.js"></script>
    <script src="/angular/controllers/authController.js"></script>
    <script src="/angular/controllers/userController.js"></script>
    <script src="/angular/controllers/taskController.js"></script>
    <script src="/angular/controllers/locationControllers.js"></script>
    <script src="/angular/services/tokenFactory.js"></script>
    <script src="/angular/services/userFactory.js"></script>
    <script src="/angular/services/taskFactory.js"></script>
    <link rel='stylesheet' href='/bower/bootstrap/dist/css/bootstrap.min.css' />
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body ng-app="nutmApp">
    <div class="container">
        <div class="page-header">
            <h3><%= title %> <small>version <%= version %></small></h3>
        </div>
        <div class="row">
            <div class="list-group col-sm-3">
                <a ui-sref="auth" class="list-group-item">Auth</a>
                <a ui-sref="task" class="list-group-item">Tasks</a>
                <a ui-sref="location" class="list-group-item">Location</a>
                <a ui-sref="user" class="list-group-item">Users</a>
            </div>
            <ui-view class="col-sm-9"></ui-view>
        </div>
    </div>
  </body>
</html>
